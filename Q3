#include <iostream>
using namespace std;
int main() {
    int percentage;
    cout << "Enter your percentage: ";
    cin >> percentage;
    switch (percentage / 10) {
        case 10: // For 100
        case 9:
            cout << "Grade: A" << endl;
            break;
        case 8:
            cout << "Grade: B" << endl;
            break;
        case 7:
            cout << "Grade: C" << endl;
            break;
        case 6:
            cout << "Grade: D" << endl;
            break;
        default:
            cout << "Grade: F" << endl;
    }

    return 0;
}
